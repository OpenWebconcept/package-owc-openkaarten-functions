{
    "name": "acato/openkaarten-base-functions",
    "type": "library",
    "description": "Adds global functions for OpenKaarten Base and OpenKaarten add-on plugins.",
    "homepage": "https://www.acato.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "Eyal Beker",
            "email": "eyal@acato.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Openkaarten_Base_Functions\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@bitbucket.org:acato/wp-pre-commit-hook.git"
        }
    ],
    "require" : {
        "composer/installers" : "~1.0"
    },
    "require-dev" : {
        "acato/wp-pre-commit-hook": "*",
        "phpcompatibility/phpcompatibility-wp": "*"
    },
    "config" : {
        "allow-plugins" : {
            "composer/installers" : true,
            "dealerdirect/phpcodesniffer-composer-installer" : true,
            "acato/wp-pre-commit-hook": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
